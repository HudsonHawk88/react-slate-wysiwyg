{
  "name": "@inftechsol/react-slate-wysiwyg",
  "version": "1.6.1",
  "description": "This is a wysiwyg editor with Slate js in React",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build-rollup": "webpack --mode production --config webpack.config.js",
    "watch": "rollup -c -w",
    "pub": "npm publish --access public",
    "test": "react-scripts test",



    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HudsonHawk88/react-slate-wysiwyg.git"
  },
  "keywords": [
    "wysiwyg",
    "react"
  ],
  "author": "Tóth Gergő",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HudsonHawk88/react-slate-wysiwyg/issues"
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "5.2.2",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-extensions": "^1.1.0",
    "is-url": "^1.2.4",
    "node-sass": "^7.0.3",
    "raw-loader": "^4.0.2",
    "reactstrap": "9.1.5",
    "sass-loader": "^13.2.0",
    "slate-history": "^0.93.0",
    "slate-hyperscript": "^0.77.0",
    "slate-react": "^0.95.0",
    "style-loader": "^3.3.1",
    "typescript-plugin-css-modules": "^4.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.11.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/bootstrap": "^5.1.11",
    "@types/escape-html": "^1.0.2",
    "@types/is-url": "^1.2.30",
    "@types/node": "^20.2.5",
    "@types/prop-types": "15.7.5",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.3",
    "prettier": "2.7.1",
    "react-scripts": "^5.0.1",
    "rollup": "^2.79.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.58.3",
    "ts-loader": "^9.4.1",
    "tsc-hooks": "^1.1.1",
    "typescript": "^4.8.4",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
